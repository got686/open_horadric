{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": false,
    "declaration": true,
    "baseUrl": "..",
    "rootDir": ".."
  }
}